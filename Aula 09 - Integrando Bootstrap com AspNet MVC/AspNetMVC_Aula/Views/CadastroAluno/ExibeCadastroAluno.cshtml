@model AspNetMVC_Aula.Models.ModAluno

@{
    ViewBag.Title = "Exibe Cadastro Aluno Tipado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="row">
    <div class="col-md-12">


        <fieldset>
            <legend>Exibe dados Aluno</legend>

            <div class="well">

                <div class="row">
                    <div class="col-md-4">
                        <strong>@Html.LabelFor(model => Model.IdAluno): </strong>
                    </div>
                    <div class="col-md-8">
                        @Html.DisplayFor(model => Model.IdAluno)
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-4">
                        <strong>@Html.LabelFor(model => Model.Nome): </strong>
                    </div>
                    <div class="col-md-8">
                        @Html.DisplayFor(model => Model.Nome)
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <strong>@Html.LabelFor(model => Model.Email): </strong>
                    </div>
                    <div class="col-md-8">
                        @Html.DisplayFor(model => Model.Email)
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <strong>@Html.LabelFor(model => Model.DtCadastro): </strong>
                    </div>
                    <div class="col-md-8">
                        @Html.DisplayFor(model => Model.DtCadastro)
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">
                        <strong>@Html.LabelFor(model => Model.ValorCurso): </strong>
                    </div>
                    <div class="col-md-8">
                        @Html.DisplayFor(model => Model.ValorCurso)
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-4">
                        <strong>@Html.LabelFor(model => Model.Senha): </strong>
                    </div>
                    <div class="col-md-8">
                        @Html.DisplayFor(model => Model.Senha)
                    </div>                    
                </div>

            </div>


        </fieldset>
    </div>
</div>